{
  "$schema": "../package.schema.json",
  "name": "hyperfine",
  "description": "A command-line benchmarking tool",
  "homepage": "https://github.com/sharkdp/hyperfine",
  "version": "1.19.0",
  "sha256": {
    "macos-arm64": "502e7c7f99e7e1919321eaa23a4a694c34b1b92d99cbd773a4a2497e100e088f"
  },
  "url": {
    "macos-arm64": "https://github.com/sharkdp/hyperfine/releases/download/v{{ version }}/hyperfine-v{{ version }}-aarch64-apple-darwin.tar.gz"
  },
  "scripts": {
    "install": {
      "macos-arm64": [
        "tar -xf hyperfine-v{{ version }}-aarch64-apple-darwin.tar.gz",
        "install hyperfine-v{{ version }}-aarch64-apple-darwin/hyperfine {{ pkg.bin_dir }}/hyperfine"
      ]
    },
    "latest": [
      "gh release list -R sharkdp/hyperfine --json tagName --jq '.[0].tagName' | cut -c 2-"
    ],
    "completions": {
      "macos-arm64": [
        "cp hyperfine-v{{ version }}-aarch64-apple-darwin/autocomplete/_hyperfine {{ pkg.completions.zsh }}/_hyperfine"
      ]
    }
  }
}
