{
  "$schema": "../package.schema.json",
  "name": "hyperfine",
  "description": "A command-line benchmarking tool",
  "homepage": "https://github.com/sharkdp/hyperfine",
  "version": "1.19.0",
  "sha256": "502e7c7f99e7e1919321eaa23a4a694c34b1b92d99cbd773a4a2497e100e088f",
  "url": "https://github.com/sharkdp/hyperfine/releases/download/v{{ version }}/hyperfine-v{{ version }}-aarch64-apple-darwin.tar.gz",
  "scripts": {
    "install": [
      "tar -xf hyperfine-v{{ version }}-aarch64-apple-darwin.tar.gz",
      "install hyperfine-v{{ version }}-aarch64-apple-darwin/hyperfine {{ pkg.bin_dir }}/hyperfine"
    ],
    "latest": [
      "git ls-remote --tags --refs --sort='v:refname' https://github.com/sharkdp/hyperfine | tail -1 | cut -d '/' -f 3 | cut -c 2-"
    ],
    "completions": [
      "cp hyperfine-v{{ version }}-aarch64-apple-darwin/autocomplete/_hyperfine {{ pkg.completions.zsh }}/_hyperfine"
    ]
  }
}
