{
  "$schema": "../package.schema.json",
  "name": "gh",
  "description": "GitHub's official command line tool",
  "homepage": "https://cli.github.com/",
  "version": "2.78.0",
  "sha256": {
    "linux-arm64": "9e3ca75b227a5503f6ef92c4b8b6dbf94e34bfdd8069ac0f16b8739856ebba7b",
    "linux-x64": "ac309f70c5d6b122c82e6138ce82cb65ca5d8595cc09d11751fbc4e3907e1a05",
    "macos-arm64": "62290b0dbc9965ebbc20d2b481fe83aa167fadc28e0d81a5d93eec6efc9d3b72",
    "macos-x64": "a8d27429a00f50ae3318fc75a9ed1e8400932c109a657e1570467eab76f6c419"
  },
  "url": {
    "linux-arm64": "https://github.com/cli/cli/releases/download/v{{ version }}/gh_{{ version }}_linux_arm64.tar.gz",
    "linux-x64": "https://github.com/cli/cli/releases/download/v{{ version }}/gh_{{ version }}_linux_amd64.tar.gz",
    "macos-arm64": "https://github.com/cli/cli/releases/download/v{{ version }}/gh_{{ version }}_macOS_arm64.zip",
    "macos-x64": "https://github.com/cli/cli/releases/download/v{{ version }}/gh_{{ version }}_macOS_amd64.zip"
  },
  "scripts": {
    "install": {
      "linux-arm64": [
        "tar -xf gh_{{ version }}_linux_arm64.tar.gz",
        "install gh_{{ version }}_linux_arm64/bin/gh {{ pkg.bin_dir }}/gh"
      ],
      "linux-x64": [
        "tar -xf gh_{{ version }}_linux_amd64.tar.gz",
        "install gh_{{ version }}_linux_amd64/bin/gh {{ pkg.bin_dir }}/gh"
      ],
      "macos-arm64": [
        "unzip gh_{{ version }}_macOS_arm64.zip",
        "install gh_{{ version }}_macOS_arm64/bin/gh {{ pkg.bin_dir }}/gh"
      ],
      "macos-x64": [
        "unzip gh_{{ version }}_macOS_amd64.zip",
        "install gh_{{ version }}_macOS_amd64/bin/gh {{ pkg.bin_dir }}/gh"
      ]
    },
    "latest": [
      "gh release list -R cli/cli --json tagName --jq '.[0].tagName' | cut -c 2-"
    ],
    "completions": {
      "linux-arm64": ["gh completion -s zsh > {{ pkg.completions.zsh }}/_gh"],
      "linux-x64": ["gh completion -s zsh > {{ pkg.completions.zsh }}/_gh"],
      "macos-arm64": ["gh completion -s zsh > {{ pkg.completions.zsh }}/_gh"],
      "macos-x64": ["gh completion -s zsh > {{ pkg.completions.zsh }}/_gh"]
    }
  }
}
