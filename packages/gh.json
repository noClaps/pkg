{
  "$schema": "../package.schema.json",
  "name": "gh",
  "description": "GitHubâ€™s official command line tool",
  "homepage": "https://cli.github.com/",
  "version": "2.76.2",
  "sha256": "43cea90fbd5d3b607b6125082f4a5faf7d5fc2e548d3d349a3b0310706a4f878",
  "url": "https://github.com/cli/cli/releases/download/v{{ version }}/gh_{{ version }}_macOS_arm64.zip",
  "scripts": {
    "install": [
      "unzip gh_{{ version }}_macOS_arm64.zip",
      "install gh_{{ version }}_macOS_arm64/bin/gh {{ pkg.bin_dir }}/gh"
    ],
    "latest": [
      "git ls-remote --tags --refs --sort='v:refname' https://github.com/cli/cli | tail -1 | cut -d '/' -f 3 | cut -c 2-"
    ],
    "completions": ["gh completion -s zsh > {{ pkg.completions.zsh }}/_gh"]
  }
}
