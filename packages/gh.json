{
  "$schema": "../package.schema.json",
  "name": "gh",
  "description": "GitHubâ€™s official command line tool",
  "homepage": "https://cli.github.com/",
  "version": "2.78.0",
  "sha256": {
    "macos-arm64": "62290b0dbc9965ebbc20d2b481fe83aa167fadc28e0d81a5d93eec6efc9d3b72"
  },
  "url": {
    "macos-arm64": "https://github.com/cli/cli/releases/download/v{{ version }}/gh_{{ version }}_macOS_arm64.zip"
  },
  "scripts": {
    "install": {
      "macos-arm64": [
        "unzip gh_{{ version }}_macOS_arm64.zip",
        "install gh_{{ version }}_macOS_arm64/bin/gh {{ pkg.bin_dir }}/gh"
      ]
    },
    "latest": [
      "gh release list -R cli/cli --json tagName --jq '.[0].tagName' | cut -c 2-"
    ],
    "completions": {
      "macos-arm64": [
        "gh completion -s zsh > {{ pkg.completions.zsh }}/_gh"
      ]
    }
  }
}
