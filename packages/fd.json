{
  "$schema": "../package.schema.json",
  "name": "fd",
  "description": "A simple, fast and user-friendly alternative to 'find'",
  "homepage": "https://github.com/sharkdp/fd",
  "version": "10.3.0",
  "sha256": {
    "linux-arm64": "66f297e404400a3358e9a0c0b2f3f4725956e7e4435427a9ae56e22adbe73a68",
    "linux-x64": "c3c2bc79f838e780173fc8f18b337ec273e7ba17c7ff8f551be29fc3c19b7916",
    "macos-arm64": "0570263812089120bc2a5d84f9e65cd0c25e4a4d724c80075c357239c74ae904",
    "macos-x64": "50d30f13fe3d5914b14c4fff5abcbd4d0cdab4b855970a6956f4f006c17117a3"
  },
  "url": {
    "linux-arm64": "https://github.com/sharkdp/fd/releases/download/v{{ version }}/fd-v{{ version }}-aarch64-unknown-linux-gnu.tar.gz",
    "linux-x64": "https://github.com/sharkdp/fd/releases/download/v{{ version }}/fd-v{{ version }}-x86_64-unknown-linux-gnu.tar.gz",
    "macos-arm64": "https://github.com/sharkdp/fd/releases/download/v{{ version }}/fd-v{{ version }}-aarch64-apple-darwin.tar.gz",
    "macos-x64": "https://github.com/sharkdp/fd/releases/download/v{{ version }}/fd-v{{ version }}-x86_64-apple-darwin.tar.gz"
  },
  "scripts": {
    "install": {
      "linux-arm64": [
        "tar -xf fd-v{{ version }}-aarch64-unknown-linux-gnu.tar.gz",
        "install fd-v{{ version }}-aarch64-unknown-linux-gnu/fd {{ pkg.bin_dir }}/fd"
      ],
      "linux-x64": [
        "tar -xf fd-v{{ version }}-x86_64-unknown-linux-gnu.tar.gz",
        "install fd-v{{ version }}-x86_64-unknown-linux-gnu/fd {{ pkg.bin_dir }}/fd"
      ],
      "macos-arm64": [
        "tar -xf fd-v{{ version }}-aarch64-apple-darwin.tar.gz",
        "install fd-v{{ version }}-aarch64-apple-darwin/fd {{ pkg.bin_dir }}/fd"
      ],
      "macos-x64": [
        "tar -xf fd-v{{ version }}-x86_64-apple-darwin.tar.gz",
        "install fd-v{{ version }}-x86_64-apple-darwin/fd {{ pkg.bin_dir }}/fd"
      ]
    },
    "latest": [
      "gh release list -R sharkdp/fd --json tagName --jq '.[0].tagName' | cut -c 2-"
    ],
    "completions": {
      "linux-arm64": [
        "cp fd-v{{ version }}-aarch64-unknown-linux-gnu/autocomplete/_fd {{ pkg.completions.zsh }}/_fd"
      ],
      "linux-x64": [
        "cp fd-v{{ version }}-x86_64-unknown-linux-gnu/autocomplete/_fd {{ pkg.completions.zsh }}/_fd"
      ],
      "macos-arm64": [
        "cp fd-v{{ version }}-aarch64-apple-darwin/autocomplete/_fd {{ pkg.completions.zsh }}/_fd"
      ],
      "macos-x64": [
        "cp fd-v{{ version }}-x86_64-apple-darwin/autocomplete/_fd {{ pkg.completions.zsh }}/_fd"
      ]
    }
  }
}
