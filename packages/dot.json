{
  "$schema": "../package.schema.json",
  "name": "dot",
  "description": "My cleaned up fork of doot that has only the features I want",
  "homepage": "https://github.com/noClaps/dot",
  "version": "0.2.5",
  "sha256": "2fbcaed7c4109dc5f01813bfd6a6cc996be061c5ee980659c6b3589ba1b429aa",
  "url": "https://github.com/noClaps/dot/releases/download/v{{ version }}/dot-darwin-arm64.tar.xz",
  "scripts": {
    "install": [
      "tar -xf dot-darwin-arm64.tar.xz",
      "install dot {{ pkg.bin_dir }}/dot"
