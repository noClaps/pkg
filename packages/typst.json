  "homepage": "https://typst.app",
  "version": "0.13.1",
  "sha256": {
    "linux-arm64": "4f5b7ee6e57fb639019ee0f6bffcf940edad228ede6ff5269a9f05a1544ceed4",
    "linux-x64": "7d214bfeffc2e585dc422d1a09d2b144969421281e8c7f5d784b65fc69b5673f",
    "macos-arm64": "541e4f9eaca3f34ee865f81fc663e4839cb84d6253f71a372cd855b0a7283213",
