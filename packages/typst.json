  },
  "url": {
    "linux-arm64": "https://github.com/typst/typst/releases/download/v{{ version }}/typst-aarch64-unknown-linux-musl.tar.xz",
    "linux-x64": "https://github.com/typst/typst/releases/download/v{{ version }}/typst-x86_64-unknown-linux-musl.tar.xz",
    "macos-arm64": "https://github.com/typst/typst/releases/download/v{{ version }}/typst-aarch64-apple-darwin.tar.xz",
    "macos-x64": "https://github.com/typst/typst/releases/download/v{{ version }}/typst-x86_64-apple-darwin.tar.xz"
  },
  "scripts": {
    "install": {
      "linux-arm64": [
        "tar -xf typst-aarch64-unknown-linux-musl.tar.xz",
        "install typst-aarch64-unknown-linux-musl/typst {{ pkg.bin_dir }}"
      ],
      "linux-x64": [
        "tar -xf typst-x86_64-unknown-linux-musl.tar.xz",
        "install typst-x86_64-unknown-linux-musl/typst {{ pkg.bin_dir }}"
      ],
      "macos-arm64": [
        "tar -xf typst-aarch64-apple-darwin.tar.xz",
        "install typst-aarch64-apple-darwin/typst {{ pkg.bin_dir }}"
      ],
      "macos-x64": [
        "tar -xf typst-x86_64-apple-darwin.tar.xz",
        "install typst-x86_64-apple-darwin/typst {{ pkg.bin_dir }}"
      ]
    },
    "latest": [
      "gh release list -R typst/typst --json tagName --jq '.[0].tagName' | cut -c 2-"
    ]
  }
}
