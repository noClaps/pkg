{
  "$schema": "../package.schema.json",
  "name": "lazygit",
  "description": "simple terminal UI for git commands",
  "homepage": "https://github.com/jesseduffield/lazygit",
  "version": "0.55.0",
  "sha256": {
    "linux-arm64": "6730250c140cd02e464f3e49b838deb4422ee61ada3227d4d88bd23fa881b8f0",
    "linux-x64": "e7c1c2d127ebdb241398f3daf05907a71a0c977ff12beba6b8ebdb069f9dba6a",
    "macos-arm64": "a3679901c0618c906424c0f9cf49abb4b687b2c610f47ff344420b9a494becf4",
    "macos-x64": "4114a044f8323b0bd2224841536103d7c6b63ddbb589242a336eb34d4ca615cf"
  },
  "url": {
    "linux-arm64": "https://github.com/jesseduffield/lazygit/releases/download/v{{ version }}/lazygit_{{ version }}_linux_arm64.tar.gz",
    "linux-x64": "https://github.com/jesseduffield/lazygit/releases/download/v{{ version }}/lazygit_{{ version }}_linux_x86_64.tar.gz",
    "macos-arm64": "https://github.com/jesseduffield/lazygit/releases/download/v{{ version }}/lazygit_{{ version }}_darwin_arm64.tar.gz",
    "macos-x64": "https://github.com/jesseduffield/lazygit/releases/download/v{{ version }}/lazygit_{{ version }}_darwin_x86_64.tar.gz"
  },
  "scripts": {
    "install": {
      "linux-arm64": [
        "tar -xf lazygit_{{ version }}_linux_arm64.tar.gz lazygit",
        "install lazygit {{ pkg.bin_dir }}/lazygit"
      ],
      "linux-x64": [
        "tar -xf lazygit_{{ version }}_linux_x86_64.tar.gz lazygit",
        "install lazygit {{ pkg.bin_dir }}/lazygit"
      ],
      "macos-arm64": [
        "tar -xf lazygit_{{ version }}_darwin_arm64.tar.gz lazygit",
        "install lazygit {{ pkg.bin_dir }}/lazygit"
      ],
      "macos-x64": [
        "tar -xf lazygit_{{ version }}_darwin_x86_64.tar.gz lazygit",
        "install lazygit {{ pkg.bin_dir }}/lazygit"
      ]
    },
    "latest": [
      "gh release list -R jesseduffield/lazygit --json tagName --jq '.[0].tagName' | cut -c 2-"
    ]
  }
}
